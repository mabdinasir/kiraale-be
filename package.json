{
  "name": "kiraale-be",
  "description": "Kiraale Backend",
  "module": "src/main.ts",
  "main": "src/main.ts",
  "engines": {
    "node": "22.x"
  },
  "type": "module",
  "version": "1.0.0",
  "scripts": {
    "build": "bun run tsc && bun run resolve-tspaths",
    "start": "bun run pm2 start dist/main.js --name kiraale-be",
    "dev": "bun --watch src/main.ts",
    "fmt": "bun run prettier --write .",
    "fmt:check": "bun run prettier --check .",
    "lint": "bun run eslint . --ext .ts,.js",
    "lint:fix": "bun run eslint . --ext .ts,.js --fix",
    "tsc": "tsc --noEmit",
    "db:generate": "drizzle-kit generate --config=src/db/drizzle.config.ts",
    "db:migrate": "drizzle-kit migrate --config=src/db/drizzle.config.ts",
    "db:studio": "drizzle-kit studio --config=src/db/drizzle.config.ts",
    "db:seed-achievements": "bun run scripts/seedAchievements.ts"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.873.0",
    "@aws-sdk/s3-request-presigner": "^3.873.0",
    "axios": "^1.11.0",
    "bcryptjs": "^3.0.2",
    "body-parser": "^2.2.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.1",
    "drizzle-kit": "^0.31.4",
    "drizzle-orm": "^0.44.4",
    "drizzle-zod": "^0.8.3",
    "express": "^5.1.0",
    "express-rate-limit": "^8.0.1",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^2.0.2",
    "node-cron": "^4.2.1",
    "nodemailer": "^7.0.5",
    "pg": "^8.16.3",
    "pm2": "^6.0.8",
    "postgres": "^3.4.7",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "tsconfig-paths": "^4.2.0",
    "zod": "^4.0.17"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@types/bun": "^1.2.20",
    "@types/cookie-parser": "^1.4.9",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/multer": "^2.0.0",
    "@types/node-cron": "^3.0.11",
    "@types/nodemailer": "^6.4.19",
    "@types/pg": "^8.15.5",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "@typescript-eslint/eslint-plugin": "^8.40.0",
    "@typescript-eslint/parser": "^8.40.0",
    "eslint": "^9.33.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-unused-imports": "^4.2.0",
    "globals": "^16.3.0",
    "nodemon": "^3.1.10",
    "prettier": "^3.6.2",
    "resolve-tspaths": "^0.8.23",
    "tsx": "^4.20.4",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.40.0"
  }
}
